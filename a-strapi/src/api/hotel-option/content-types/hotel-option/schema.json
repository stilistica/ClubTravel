{
  "kind": "collectionType",
  "collectionName": "hotel_options",
  "info": {
    "singularName": "hotel-option",
    "pluralName": "hotel-options",
    "displayName": "HotelOptions"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "tourPackage": {
      "type": "enumeration",
      "enum": [
        "Туристический пакет",
        "package",
        "Только перелет",
        "flight_only"
      ]
    },
    "price": {
      "type": "integer"
    },
    "meals": {
      "type": "enumeration",
      "enum": [
        "Без питания",
        "no_meals",
        "Завтрак",
        "breakfast",
        "Затрак и ужин",
        "breakfast_dinner",
        "Завтрак, обед, ужин",
        "full_board",
        "Всё включено",
        "all_inclusive",
        "Ультра: всё включено",
        "ultra_all_inclusive"
      ]
    },
    "hotels": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::hotel.hotel",
      "inversedBy": "hotel_options"
    }
  }
