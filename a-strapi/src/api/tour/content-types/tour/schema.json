    },
    "destination": {
      "type": "enumeration",
      "enum": [
        "Египет",
        "АОЭ",
        "Тайланд",
        "Болгария",
        "Тенерифе",
        "Бали",
        "Грузия",
        "Греция",
        "Турция",
        "Кипр"
      ]
    },
    "tour_options": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tour-option.tour-option",
      "mappedBy": "tour"
    },
    "regions": {
      "type": "component",
      "component": "tour-components.regions",
      "repeatable": true
    }
  }
}
